<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_appsetting"
    app:startDestination="@+id/appSettingFragment">
    <fragment
        android:id="@+id/appSettingFragment"
        android:name="com.lotuss.oneapp.profile.appsetting.AppSettingFragment">
        <action
            android:id="@+id/action_appSettingFragment_to_verifyUserFragment"
            app:destination="@+id/nav_verifyuser">
            <argument
                android:name="actionName"
                app:argType="string" />
        </action>

        <action
            android:id="@+id/action_appSettingFragment_to_webContentFragment"
            app:destination="@+id/webContentFragment" />
    </fragment>

    <fragment
        android:id="@+id/webContentFragment"
        android:name="com.lotuss.oneapp.profile.webcontent.WebContentFragment">
        <argument
            android:name="webContent"
            app:argType="com.lotuss.oneapp.domain.model.WebContent" />
    </fragment>
</navigation>